{
  "name": "vql",
  "version": "1.0.0",
  "description": "Vizi querying language",
  "scripts": {
    "build": "npm run build-cjs && npm run build-es6 && rimraf umd && npm run build-umd && npm run build-min",
    "build-cjs": "rimraf lib && babel ./src -d lib",
    "build-es6": "rimraf es6 && babel ./src -d es6 --blacklist=es6.modules",
    "build-umd": "webpack --env=development src/index.js umd/npm-package-template.js",
    "build-min": "webpack --env=production src/index.js umd/npm-package-template.min.js",
    "lint": "eslint src",
    "test": "mocha --recursive tests",
    "start": "npm run build-cjs && node demo/repl.js"
  },
  "main": "lib/index",
  "jsnext:main": "es6/index",
  "engines": {
    "node": ">=6.9.x",
    "yarn": ">=0.18.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vizi-Dashboard/vql.git"
  },
  "files": [
    "*.md",
    "es6/*",
    "es6/*/**",
    "lib/*",
    "lib/*/**",
    "umd/*",
    "umd/*/**",
    "src/*",
    "src/*/**"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/Vizi-Dashboard/vql",
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "eslint": "^3.16.1",
    "mocha": "^3.2.0",
    "readline": "^1.3.0",
    "rimraf": "^2.6.1",
    "webpack": "^2.2.1"
  }
}
